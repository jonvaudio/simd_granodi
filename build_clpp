clang++ -o test_generic_debug test_simd_granodi.c -Wall -Wextra -std=c++11 -D SIMD_GRANODI_FORCE_GENERIC -lm
clang++ -o test_generic_opt test_simd_granodi.c -Wall -Wextra -std=c++11 -D NDEBUG -D SIMD_GRANODI_FORCE_GENERIC -O3 -lm
clang++ -o test_sse2neon_debug test_simd_granodi.c -Wall -Wextra -std=c++11
clang++ -o test_sse2neon_opt test_simd_granodi.c -Wall -Wextra -std=c++11 -D NDEBUG -O3
