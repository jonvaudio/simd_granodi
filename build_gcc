# We do -lm on all builds now as ARM needs it in GCC
gcc -o test_generic_debug test_simd_granodi.c -Wall -Wextra -std=c99 -D SIMD_GRANODI_FORCE_GENERIC -lm
gcc -o test_generic_opt test_simd_granodi.c -Wall -Wextra -std=c99 -D NDEBUG -D SIMD_GRANODI_FORCE_GENERIC -O3 -lm
gcc -o test_sse2neon_debug test_simd_granodi.c -Wall -Wextra -std=c99 -lm
gcc -o test_sse2neon_opt test_simd_granodi.c -Wall -Wextra -std=c99 -D NDEBUG -O3 -lm
